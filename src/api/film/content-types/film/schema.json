{
  "kind": "collectionType",
  "collectionName": "films",
  "info": {
    "singularName": "film",
    "pluralName": "films",
    "displayName": "Film"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "Film"
    },
    "released": {
      "type": "date"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
