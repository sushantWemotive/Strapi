{
  "kind": "collectionType",
  "collectionName": "addresses",
  "info": {
    "singularName": "address",
    "pluralName": "addresses",
    "displayName": "address"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "type": {
      "type": "enumeration",
      "enum": ["home", "work", "other"]
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string",
      "required": true
    },
    "addressLine1": {
      "type": "string",
      "required": true
    },
    "addressLine2": {
      "type": "string"
    },
    "city": {
      "type": "string",
      "required": true
    },
    "state": {
      "type": "string",
      "required": true
    },
    "postalCode": {
      "type": "string",
      "required": true
    },
    "isDefault": {
      "type": "boolean",
      "default": false
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::e-user.e-user",
      "inversedBy": "addresses"
    }
  }
}